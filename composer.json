    "description": "Automatically cast Eloquent columns to MoneyPHP objects.",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Christian Griffin",
            "email": "christian.griffin@live.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "moneyphp/money": "^3.2"
    },
    "require-dev": {
        "illuminate/database": "^5.6",
        "phpunit/phpunit": "^7.5",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
